<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="400.0" maxWidth="200.0" minHeight="400.0" minWidth="200.0" prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xyz.kholdy.smartlight.Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="200.0">
         <children>
            <ToggleButton fx:id="btnHallway" mnemonicParsing="false" onAction="#handlerBtnHallway" prefWidth="100.0" text="Hallway">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="btnKitchen" mnemonicParsing="false" onAction="#handlerBtnKitchen" prefWidth="100.0" text="Kitchen">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="btnBedroom" mnemonicParsing="false" onAction="#handlerBtnBedroom" prefWidth="100.0" text="Bedroom">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="btnLivingRoom" mnemonicParsing="false" onAction="#handlerBtnLivingRom" prefWidth="100.0" text="Living room">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ToggleButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
